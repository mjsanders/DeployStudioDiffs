<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.cvmsServ</string>
	<key>HopefullyExitsLast</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/bin/sandbox-exec</string>
		<string>-f</string>
		<string>/usr/share/sandbox/cvmsServer.sb</string>
		<string>/System/Library/Frameworks/OpenGL.framework/Versions/A/Libraries/cvmsServ</string>
	</array>
	<key>MachServices</key>
	<dict>
		<key>com.apple.cvmsServ</key>
		<true/>
	</dict>
	<key>EnableTransactions</key>
	<true/>
</dict>
</plist>
